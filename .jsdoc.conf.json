{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "." ],
        "exclude": [ "node_modules" ]
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags": ["summary"]
    },
    "templates": {
        "cleverLinks": true,
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "template": "jsdoc-template",
        "recurse": true,
        "verbose": true,
        "destination": "./doc/",
        "private": false,
        "readme": "README.md"
    }
}
